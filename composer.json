{
    "name": "cnect/futurium8",
    "description": "Futurium 8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "balbuf/drupal-libraries-installer": "^1.0",
        "boedah/robo-drush": "~4",
        "cnect-web/blellow": "dev-develop",
        "cnect-web/group_permissions": "1.0",
        "composer/installers": "dev-master as 1.6.0",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/address": "^1.6",
        "drupal/admin_toolbar": "^1.26",
        "drupal/adminimal_theme": "^1.4",
        "drupal/allowed_formats": "^1.1",
        "drupal/better_normalizers": "^1.0",
        "drupal/chosen": "^2.7",
        "drupal/coffee": "^1.0",
        "drupal/components": "^1.0",
        "drupal/config_readonly": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.9",
        "drupal/core": "^8.7",
        "drupal/ctools": "^3.0",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/extra_field": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/fontawesome": "^2.1",
        "drupal/fontawesome_menu_icons": "^1.2",
        "drupal/geocoder": "^2.1",
        "drupal/geofield": "^1.1",
        "drupal/group": "1.x-dev",
        "drupal/honeypot": "^1.29",
        "drupal/hook_event_dispatcher": "^1.25",
        "drupal/inline_entity_form": "^1.0",
        "drupal/leaflet": "^1.4",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/permissions_filter": "^1.0",
        "drupal/redirect": "^1.3",
        "drupal/search_api": "^1.0",
        "drupal/single_datetime": "^1.7",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/token": "^1.5",
        "drupal/twig_extensions": "^2.0",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^2.0",
        "drupal/ui_patterns": "^1.0",
        "drush/drush": "^9.0",
        "harvesthq/chosen": "^1.8",
        "nuvoleweb/robo-config": "^0.2",
        "openeuropa/ecl-twig-loader": "~0.1",
        "skounis/blellow_helper": "dev-master",
        "twig/extensions": "^1.5",
        "twig/twig": "^1.38.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "1.7.x-dev",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "drupal/config_devel": "^1.2",
        "drupal/default_content_deploy": "^1.0@alpha",
        "drupal/devel": "^2.0",
        "drupal/devel_php": "^1.1",
        "drupal/drupal-driver": "~2.0",
        "drupal/drupal-extension": "v4.0.0beta2",
        "drupal/search_kint": "^1.0",
        "guzzlehttp/guzzle": "^6.0@dev",
        "horaciolopesqq/docker": "0.0.7",
        "skounis/fut_default_content": "dev-master",
        "skounis/fut_sample_content": "dev-master",
        "webflo/drupal-core-require-dev": "~8.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "fut\\Robo\\": "robo"
        },
        "classmap": [
            "resources/scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "bash resources/scripts/composer/symlink.sh"
        ],
        "post-update-cmd": []
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-libraries": {
            "datetimepicker": "https://github.com/xdan/datetimepicker/archive/2.5.20.zip"
         },
        "_comment_patches": "Temporarily enabling patches on chained composer.json until config_installer is on core",
        "patches": {
            "enable-patching" : true,
            "drupal/core": {
                "LanguageNegotiator::updateConfiguration() is passed a NULL": "https://www.drupal.org/files/issues/2019-02-28/language-2718657-17.patch"
            },
            "drupal/views": {
                "Cannot use relationship for rendered entity on Views": "https://www.drupal.org/files/issues/2019-03-15/2457999-189.patch"
            },
            "drupal/group": {
                "Port Subgroup (ggroup) to the D8 version": "https://www.drupal.org/files/issues/2019-06-15/port-subgroups-2736233-236.patch",
                "Request membership feature": "https://www.drupal.org/files/issues/2019-03-11/group-request_membership_feature-2752603-88.patch",
                "How to redirect to the owning group after adding a gnode?": "https://www.drupal.org/files/issues/2817109-by-rachel_norfolk-ericras-How-to-redir.patch",
                "Wizard duplicates membership": "https://www.drupal.org/files/issues/2019-07-09/group_membership_duplication-3016197-10.patch",
                "Get a token of a node's parent group to create a pathauto pattern" : "https://www.drupal.org/files/issues/group_add-node-group-tokens-2774827-24.patch",
                "Media enabler" : "https://www.drupal.org/files/issues/2019-03-01/media-enabler-2984315-22.patch",
                "Group language settings are missing" : "https://www.drupal.org/files/issues/2019-02-27/group_type_language_settings-3036272-2.patch",
                "Can't translate group roles" : "https://www.drupal.org/files/issues/2019-03-07/group_role_translations-3014651-3.patch",
                "Behat group context": "./resources/patches/group.behat.inc.patch"
            },
            "drupal/inline_entity_form": {
                "Remove revision log messages from other entities than nodes": "https://www.drupal.org/files/issues/2018-03-23/remove_revision_log_message-2927884-2.patch"
            },
            "drupal/message": {
                "Notice: Undefined index: #cardinality": "https://www.drupal.org/files/issues/2018-04-04/fix_misspelling_in_cardinality_var-2958485.txt"
            },
            "drupal/default_content": {
                "Handle exceptions during import": "https://www.drupal.org/files/issues/2019-05-29/3057880-handleexceptions.patch"
            },
            "drupal/default_content_deploy": {
                "Pass logger to parent": "https://www.drupal.org/files/issues/2019-07-08/passloggertoparent-3066432.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
          "type": "package",
          "package": {
            "name": "harvesthq/chosen",
            "version": "1.8.7",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
              "type": "zip"
            },
            "require": {
                "composer/installers": "^1.2.0"
            }
          }
        },
        {
          "url": "https://github.com/horaciolopesqq/installers",
          "type": "git"
        },
        {
          "type": "package",
          "package": {
            "name": "cnect-web/group_permissions",
            "version": "1.0",
            "type": "drupal-module",
            "dist": {
              "url": "https://github.com/cnect-web/group_permissions/archive/1.0.zip",
              "type": "zip"
            }
          }
        }
    ]
}
